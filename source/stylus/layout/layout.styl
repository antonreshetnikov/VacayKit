*
  pointer-events none

html
  width 100%
  height 100%
  overflow-x hidden
  font-family 'Lato', sans-serif
  body
    min-height 100%
    width 100%
    overflow-x hidden
    a,
    button,
    input,
    textarea,
    select,
    h1,
    h2,
    h3,
    h4,
    h5,
    img
      pointer-events all
